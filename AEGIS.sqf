				[_object,_building] call BIS_fnc_attachToRelative;
				_objects pushBack [_object,_pos];
			}forEach _compData;

			_building setVariable ["MAZ_EZM_CompObjects",_objects,true];
			_building setVariable ["MAZ_EZM_hasCompSetup",true,true];

