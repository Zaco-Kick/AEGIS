					private _activeChildren = [];
					{
						_x params ["","","_conditionChild"];
						if(_entity call _conditionChild) then {
							_activeChildren pushBack _x;
						};
					}forEach _childActions;
					_ctrl setVariable ["contextMenuChildrenData",_activeChildren];

